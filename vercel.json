{
  "version": 2,
  "functions": {
    "server/server-simple.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/ping",
      "dest": "/server/server-simple.js"
    },
    {
      "src": "/warmup",
      "dest": "/server/server-simple.js"
    },
    {
      "src": "/health",
      "dest": "/server/server-simple.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server/server-simple.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server/server-simple.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
